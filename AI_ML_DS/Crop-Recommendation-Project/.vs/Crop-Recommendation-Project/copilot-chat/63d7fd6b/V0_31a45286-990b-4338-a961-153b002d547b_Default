            foreach (var idx in labelIdx)
            {
                selectQuery = "Select * from idxlbcropdb where label_idx = " + idx + ";";
                reader = dbConnector.Read(selectQuery);

                dt = new DataTable();
                dt.Load(reader);

                foreach (DataColumn column in dt.Columns)
                {
                    if (column.DataType.Name != "Double")
                    {
                        continue;
                    }

